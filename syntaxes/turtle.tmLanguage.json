{
    "scopeName": "source.turtle",
    "patterns":
    [
        {
            "name": "comment.line.turtle",
            "match": "#.*$"
        },
        {
            "name": "keyword.operator.turtle",
            "match": "\\+|-|\\*|/|\\^"
        },
        {
            "name": "keyword.control.turtle",
            "match": "\\b(color|repeat|proc|call|set|quit|exit)\\b"
        },
        {
            "name": "constant.numeric.turtle",
            "match": "\\b((0(x|X)[0-9a-fA-F]*)|(([0-9]+\\.?[0-9]*)|(\\.[0-9]+))((e|E)(\\+|-)?[0-9]+)?)\\b"
        },
        {
            "name": "constant.language.turtle",
            "match": "\\b(red|green|blue|cyan|magenta|yellow|black|gray|white|north|south|east|west)\\b"
        },
        {
            "name": "string.turtle",
            "match": "(\\b[A-Z][A-Z0-9]*\\b|\\b[a-zA-Z_][a-zA-Z0-9_]*\\b)"
        }
    ],
    "repository": {}
}