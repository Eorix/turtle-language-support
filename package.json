{
    "name": "turtle-language-support",
    "displayName": "Turtle Language Support",
    "version": "1.0.0",
    "description": "Provides syntax highlighting for the Turtle language in VSCode.",
    "author": "Valentin Pinsello",
    "publisher": "Valentin Pinsello",
    "icon": "icon.png",
    "license": "CC-BY-4.0",
    "engines":
    {
        "vscode": "^1.60.0"
    },
    "categories":
    [
        "Programming Languages"
    ],
    "contributes":
    {
        "languages":
        [
            {
                "id": "turtle",
                "extensions": [".turtle"],
                "aliases": ["Turtle"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars":
        [
            {
                "language": "turtle",
                "scopeName": "source.turtle",
                "path": "./syntaxes/turtle.tmLanguage.json"
            }
        ]
    },
    "repository":
    {
        "type": "git",
        "url": "https://github.com/Eorix/turtle-language-support"
    }
}